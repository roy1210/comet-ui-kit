{
  "name": "comet-ui-kit",
  "version": "0.1.3",
  "license": "MIT",
  "module": "dist/comet-ui-kit.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "start-storybook -p 6006",
    "start": "tsdx watch",
    "build": "tsdx build && build-storybook -c .storybook -o public",
    "storybook": "start-storybook",
    "test": "tsdx test --passWithNoTests",
    "lint": "yarn format && eslint . --fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json,md,mdx,html}'",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build && yarn pretty-quick --staged",
    "prepare": "husky install",
    "push": "git push origin HEAD && yarn publish"
  },
  "size-limit": [
    {
      "path": "dist/comet-ui-kit.cjs.production.min.js"
    },
    {
      "path": "dist/comet-ui-kit.esm.js"
    }
  ],
  "dependencies": {
    "@tippyjs/react": "^4.2.0",
    "crypto-js": "^4.0.0",
    "debug": "^4.3.2",
    "nanoid": "3.1.18",
    "qrcode.react": "^1.0.0",
    "react-toastify": "^6.2.0"
  },
  "peerDependencies": {
    "@types/styled-components": "^5.1.0",
    "polished": "^4.0.0",
    "react": ">=16",
    "react-spring": "^8.0.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@rollup/plugin-url": "^5.0.1",
    "@size-limit/preset-big-lib": "^4.9.0",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/addons": "^6.1.2",
    "@storybook/react": "^6.1.2",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@types/crypto-js": "^4.0.1",
    "@types/debug": "^4.1.7",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "babel-loader": "^8.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.0",
    "polished": "^4.0.4",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-spring": "^8.0.27",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-string": "^3.0.0",
    "semantic-release": "^17.3.0",
    "size-limit": "^4.9.0",
    "styled-components": "^5.2.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "**/react": "17.0.1",
    "**/@typescript-eslint/eslint-plugin": "4.8.1",
    "**/nanoid": "3.1.18"
  }
}
